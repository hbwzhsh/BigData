jdbc:phoenix:hadoop-2.certus.com,hadoop-7.certus.com,hadoop-6.certus.com

CREATE TABLE PHOENIXHTTPINFO(
    IMSI VARCHAR not null,
    COLLECTTIME BIGINT not null,
    F.SRCADDRTYPE CHAR(1),
    F.SRCADDRHIGH BIGINT,
    F.SRCADDRLOW BIGINT,
    F.SRCPORT INTEGER,
    F.DSTADDRTYPE CHAR(1),
    F.DSTADDRHIGH BIGINT,
    F.DSTADDRLOW BIGINT,
    F.DSTPORT INTEGER,
    F.UEADDRTYPE CHAR(1),
    F.UEADDRHIGH BIGINT,
    F.UEADDRLOW BIGINT,
    F.HTTPHOST VARCHAR,
    F.CONTENTTYPE VARCHAR,
    F.MINRESPTIME INTEGER,
    F.MAXRESPTIME INTEGER,
    F.AVERRESPTIME INTEGER,
    F.STATUSCODES VARCHAR,
    F.REQUEST_COUNT INTEGER,
    F.RESPONSE_COUNT INTEGER,
    F.BRAND VARCHAR,
    F.BROWSER VARCHAR,
    F.DEVICEOS VARCHAR,
    F.SAC INTEGER
    CONSTRAINT pk PRIMARY KEY (IMSI,COLLECTTIME)) TTL='172800', COMPRESSION='SNAPPY', DEFAULT_COLUMN_FAMILY='F', IMMUTABLE_ROWS=true, DISABLE_WAL=true, SALT_BUCKETS = 60;

